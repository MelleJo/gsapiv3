{
  "functions": {
    "src/app/api/transcribe/route.ts": {
      "maxDuration": 300,
      "memory": 3008
    },
    "src/app/api/transcribe-segment/route.ts": {
      "maxDuration": 120,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache"
        }
      ]
    }
  ],
  "regions": ["iad1"],
  "github": {
    "silent": true
  }
}
