{
  "functions": {
    "src/app/api/upload-blob/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/client-upload/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
